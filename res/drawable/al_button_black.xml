<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="@dimen/al_button_stroke" android:color="#ff000000" />
            <solid android:width="0.5dip" android:color="@color/al_back_2" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="@dimen/al_button_stroke" android:color="#ff000000" />
            <solid android:width="0.5dip" android:color="@color/Transparent" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
</selector>